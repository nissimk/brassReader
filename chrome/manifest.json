{
  "manifest_version": 2,
  "name": "BrassReader",
  "description": "RSS Reader.",
  "version": "0.1",
  "icons": {
    "48": "img/star-48.png",
    "128": "img/star.png"
  },
  "app": {
    "launch": {
      "local_path": "index.html"
    }
  },
  "permissions": ["cookies", "http://*/*", "https://*/*", "storage", "unlimitedStorage"],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "offline_enabled": true
}
